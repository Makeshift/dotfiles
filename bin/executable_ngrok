#!/bin/bash
set -a
x86_64_ngrok_INSTALLER_ARCHIVE="https://bin.equinox.io/c/bNyj1mQVY4c/ngrok-v3-stable-linux-amd64.tgz"
x86_64_ngrok_INSTALLER_ARCHIVE_BINARY="ngrok"
x86_64_ngrok_INSTALLER_ARCHIVE_INSTALL_LOCATION="/usr/local/bin/ngrok"
x86_64_ngrok_INSTALLER_ARCHIVE_EXTRACT_COMMAND="tar -xzf"

armv7l_ngrok_INSTALLER_ARCHIVE="https://bin.equinox.io/c/bNyj1mQVY4c/ngrok-v3-stable-linux-arm.tgz"
armv7l_ngrok_INSTALLER_ARCHIVE_BINARY="$x86_64_ngrok_INSTALLER_ARCHIVE_BINARY"
armv7l_ngrok_INSTALLER_ARCHIVE_INSTALL_LOCATION="$x86_64_ngrok_INSTALLER_ARCHIVE_INSTALL_LOCATION"
armv7l_ngrok_INSTALLER_ARCHIVE_EXTRACT_COMMAND="$x86_64_ngrok_INSTALLER_ARCHIVE_EXTRACT_COMMAND"

aarch64_ngrok_INSTALLER_ARCHIVE="https://bin.equinox.io/c/bNyj1mQVY4c/ngrok-v3-stable-linux-arm64.tgz"
aarch64_ngrok_INSTALLER_ARCHIVE_BINARY="$x86_64_ngrok_INSTALLER_ARCHIVE_BINARY"
aarch64_ngrok_INSTALLER_ARCHIVE_INSTALL_LOCATION="$x86_64_ngrok_INSTALLER_ARCHIVE_INSTALL_LOCATION"
aarch64_ngrok_INSTALLER_ARCHIVE_EXTRACT_COMMAND="$x86_64_ngrok_INSTALLER_ARCHIVE_EXTRACT_COMMAND"

armhf_ngrok_INSTALLER_ARCHIVE="$aarch64_ngrok_INSTALLER_ARCHIVE"
armhf_ngrok_INSTALLER_ARCHIVE_BINARY="$aarch64_ngrok_INSTALLER_ARCHIVE_BINARY"
armhf_ngrok_INSTALLER_ARCHIVE_INSTALL_LOCATION="$aarch64_ngrok_INSTALLER_ARCHIVE_INSTALL_LOCATION"
armhf_ngrok_INSTALLER_ARCHIVE_EXTRACT_COMMAND="$aarch64_ngrok_INSTALLER_ARCHIVE_EXTRACT_COMMAND"

$HOME/bin/run_or_get_app ngrok "$@"
