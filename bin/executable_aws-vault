#!/bin/bash
set -a
x86_64_aws_vault_INSTALLER_BINARY="https://github.com/99designs/aws-vault/releases/download/v6.6.0/aws-vault-linux-amd64"
x86_64_aws_vault_INSTALLER_BINARY_INSTALL_LOCATION="/usr/local/bin/aws-vault"

armv7l_aws_vault_INSTALLER_BINARY="https://github.com/99designs/aws-vault/releases/download/v6.6.0/aws-vault-linux-arm7"
armv7l_aws_vault_INSTALLER_BINARY_INSTALL_LOCATION="/usr/local/bin/aws-vault"

aarch64_aws_vault_INSTALLER_BINARY="https://github.com/99designs/aws-vault/releases/download/v6.6.0/aws-vault-linux-arm64"
aarch64_aws_vault_INSTALLER_BINARY_INSTALL_LOCATION="/usr/local/bin/aws-vault"

armhf_aws_vault_INSTALLER_BINARY="$aarch64_aws_vault_INSTALLER_BINARY"
armhf_aws_vault_INSTALLER_BINARY_INSTALL_LOCATION="$aarch64_aws_vault_INSTALLER_BINARY_INSTALL_LOCATION"

"$HOME"/bin/run_or_get_app aws-vault "$@"
